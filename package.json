{
  "name": "prisma-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm --workspace apps/prisma-map run dev",
    "build": "npm --workspace apps/prisma-map run build",
    "preview": "npm --workspace apps/prisma-map run preview",
    "lint": "npm --workspace apps/prisma-map run lint",
    "typecheck": "npm --workspace apps/prisma-map run typecheck"
  },
  "devDependencies": {
    "typescript": "^5.6.0"
  }
}
